{
  "name": "express-skeleton-3",
  "version": "1.0.0",
  "description": "A Skeleton Application",
  "engines": {
    "node": ">=14"
  },
  "main": "bin/www",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "lint": "npm run lint:quiet",
    "lint:quiet": "eslint --quiet './**/*.js'",
    "lint:all": "eslint './**/*.js'",
    "lint:fix": "eslint './**/*.js' --quiet --fix",
    "test:nocoverage": "ava",
    "test:withcoverage": "nyc ava",
    "test": "npm run test:withcoverage",
    "gendoc": "apidoc -i routes/ -o public/docs"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.645.0",
    "agenda": "^4.2.1",
    "aws-sdk": "^2.1691.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "ejs": "^2.7.4",
    "email-templates": "^8.0.10",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.16.2",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.2",
    "mongodb": "^4.11.0",
    "mongodb-memory-server": "^3.1.3",
    "mongoose": "^5.10.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mustache": "^4.2.0",
    "randomstring": "^1.2.2",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.4.1",
    "socket.io-redis": "^6.1.1",
    "stripe": "^8.156.0",
    "twilio": "^5.3.0"
  },
  "eslintIgnore": [
    "public/",
    "bin/"
  ],
  "devDependencies": {
    "apidoc": "^0.17.7",
    "ava": "^3.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.22.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "nodemon": "^2.0.4",
    "nyc": "^14.1.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:quiet"
    }
  }
}
